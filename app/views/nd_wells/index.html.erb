<h1>All wells in North Dakota</h1>

<% @wells.each do |well| %>
  <a href="/nd/<%= well.id %>"><h3>File No: <%= well.id %></h3></a>
  <p>Well name: <%= well.well_name %></p>
  <p>Operator: <%= well.operator %></p>
  <p>Spud date: <%= well.spud_date %></p>
  <p>Well depth: <%= well.td %></p>
  <p>Latitude: <%= well.latitude %></p>
  <p>Longitude: <%= well.longitude %></p>
  
<% end %>